<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:fiorital="it.fiorital.fioritalui5lib.controls"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:tnt="sap.tnt" xmlns:variant="sap.ui.comp.variants">
	<HBox>
		<Button icon="sap-icon://Chart-Tree-Map" press="onPersoButtonPressed" tooltip="varianti per tabella pivot"/>
		<HBox width="1.5em"></HBox>
		<fiorital:FioritalPivotVariantManagement variantItems="{path:'/',suspended: true}" enabled="true" showExecuteOnSelection="false"
			showShare="false" id="Variants">
			<fiorital:variantItems>
				<variant:VariantItem text="{Name}" key="{Key}"/>
			</fiorital:variantItems>
		</fiorital:FioritalPivotVariantManagement>
	</HBox>
</core:FragmentDefinition>