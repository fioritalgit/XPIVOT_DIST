													<core:CustomData key="CLUSTEROK" value="{SUPPLYJSONMODEL>overAllocation}" writeToDom="true"/>
													<core:CustomData key="reacheablecluster" value="{SUPPLYJSONMODEL>reacheablecluster}" writeToDom="true"/>
