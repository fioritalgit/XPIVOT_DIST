						<HBox height="8%" alignItems="Center">
							<VBox width="1%"></VBox>
							<VBox width="29%">
								<HBox width="100%">
									<core:Icon src="sap-icon://example" size="2.5em"></core:Icon>
									<VBox width="1em"></VBox>
									<VBox>
										<Text text="General Availability" class="hdrAllocation1"></Text>
										<Text text="all products"></Text>
									</VBox>
								</HBox>
							</VBox>
							<VBox width="12.5%">
