																<VBox width="5%" justifyContent="Center"></VBox>
																<VBox width="60%" justifyContent="Center" class="padright05">
																	<ObjectIdentifier title="{WHSITEMS>node}" text="{WHSITEMS>nodedescr}" titleActive="true" titlePress=".selectWarehouse"/>
																</VBox>
																<VBox width="35%">
																	<HBox>
																		<Text text="box:" class="labelGreySmall" width="2.5em"></Text>
																		<Text text="{path:'WHSITEMS>totalCP',formatter:'.truncateNumber'}"></Text>
																	</HBox>
																	<HBox>
																		<Text text="KG:" class="labelGreySmall" width="2.5em"></Text>
																		<Text text="{path:'WHSITEMS>totalKg',formatter:'.truncateNumber'}"></Text>
