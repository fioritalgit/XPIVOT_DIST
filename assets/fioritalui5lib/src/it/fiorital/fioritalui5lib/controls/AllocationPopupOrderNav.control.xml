								<VBox height="0.2em"></VBox>
								<Switch id="onlylocalavailability" state="true" customTextOn="Yes" customTextOff="No" change=".changeLocalFlag"></Switch>
							</VBox>
							<VBox width="12.5%">
								<Text text="Only reserved availabilities"></Text>
								<VBox height="0.2em"></VBox>
								<Switch id="onlyPartner" state="false" customTextOn="Yes" customTextOff="No" change=".changeBPFlag"></Switch>
							</VBox>
							<VBox width="5%"></VBox>
							<HBox width="45%" height="100%">
								<HBox width="40%" height="100%" alignItems="Center">
									<core:Icon src="sap-icon://shipping-status" size="2.5em" press="showMainFO"></core:Icon>
									<VBox width="1em"></VBox>
									<VBox>
										<Text text="{SUPPLYJSONMODELBASE>/zzlgort}" class="hdrAllocation1"></Text>
										<Text text="{SUPPLYJSONMODELBASE>/storagelocationdescr}"></Text>
										<Text text="@ {SUPPLYJSONMODELBASE>/fodeparturedate} h: {SUPPLYJSONMODELBASE>/fodeparturetime}"></Text>
									</VBox>
								</HBox>
								<HBox id="alternativeRouteShow" width="60%" height="100%" alignItems="Center">
									<VBox>
										<core:Icon src="sap-icon://journey-change" size="1em" color="red"></core:Icon>
										<core:Icon src="sap-icon://shipping-status" size="1em" color="red"></core:Icon>
									</VBox>
									<HBox width="0.5em"></HBox>
									<VBox>
										<HBox>
											<fiorital:FioritalText text="{ parts: ['SELECTEDALTERNATIVEROUTE>/freightorderid'], formatter: '.deleteTrailZeros' }" press=".showFO"/>
											<HBox width="0.5em"></HBox>
											<Label
												text="{SELECTEDALTERNATIVEROUTE>/sotradelgort} - ({SELECTEDALTERNATIVEROUTE>/sotradedate} @{ parts: ['SELECTEDALTERNATIVEROUTE>/sotradetime'], formatter: '.formatFOtime' })"></Label>
										</HBox>
										<Text text="{SELECTEDALTERNATIVEROUTE>/carrieridname}"></Text>
									</VBox>
								</HBox>
							</HBox>
						</HBox>
						<HBox renderType="Bare" height="92%">
							<VBox renderType="Bare" width="15%" height="100%">
								<NavContainer width="100%" id="masterNavContainer" class="bordernav" height="100%">
									<Page showFooter="false" showHeader="false" id="TradeItemList">
										<VBox width="100%" height="100%" renderType="Bare">
											<VBox height="25%" width="100%" renderType="Bare" alignItems="Center" class="backcolorcadet">
												<VBox width="0.5em"></VBox>
												<!--<Text text="Trade Items" class="textBlock colortextwhite"></Text>-->
												<SearchField width="93%" id="tradeitemFilter" search=".goFilterItems" class="sapUiSmallMarginTop" placeholder="filtra codice"/>
												<HBox width="93%" renderType="Bare">
													<VBox width="30%" renderType="Bare" justifyContent="Center">
														<Label text="origine" class="colortextwhite sapUiTinyMarginBegin"></Label>
													</VBox>
													<VBox width="70%" renderType="Bare">
														<ComboBox placeholder="origine" id="filterOrigin" showSecondaryValues="true" change=".goChangeOrigin" loadItems=".loadItemsCombo"
															items="{ path: '/CharPossValues', length: 1000, filters: [{path: 'characteristicid', operator: 'EQ', value1: 'Z_PROVENIENZA'}], templateShareable: true, suspended: true}">
															<core:ListItem key="{valueid}" text="{valueid}" additionalText="{valuedescription}"/>
														</ComboBox>
													</VBox>
												</HBox>
												<HBox width="93%" renderType="Bare">
													<VBox width="30%" renderType="Bare" justifyContent="Center">
														<Label text="zona FAO" class="colortextwhite sapUiTinyMarginBegin"></Label>
													</VBox>
													<VBox width="70%" renderType="Bare">
														<ComboBox placeholder="FAO" id="filterFaozone" showSecondaryValues="true" change=".goChangeFAO" loadItems=".loadItemsCombo"
															items="{ path: '/CharPossValues', length: 1000, filters: [{path: 'characteristicid', operator: 'EQ', value1: 'Z_ZONA_FAO'}], templateShareable: true, suspended: true}">
															<core:ListItem key="{valueid}" text="{valueid}" additionalText="{valuedescription}"/>
														</ComboBox>
													</VBox>
												</HBox>
												<HBox width="93%" renderType="Bare">
													<VBox width="30%" renderType="Bare" justifyContent="Center">
														<Label text="origin PO" class="colortextwhite sapUiTinyMarginBegin"></Label>
													</VBox>
													<VBox width="70%" renderType="Bare">
														<Input placeholder="origin PO" id="filterOriginPO" change=".goChangeOriginPO"></Input>
													</VBox>
												</HBox>
												<HBox width="93%" renderType="Bare">
													<VBox width="30%" renderType="Bare" justifyContent="Center">
														<Label text="categoria" class="colortextwhite sapUiTinyMarginBegin"></Label>
													</VBox>
													<VBox width="70%" renderType="Bare">
														<ComboBox placeholder="categoria prodotto" id="filterProductCategory" showSecondaryValues="true" change=".goChangeProductCategory" loadItems=".loadItemsCombo"
															items="{ path: '/ProductCategory', $select:'settore,settoredescr', length: 1000, templateShareable: true, suspended: true}">
															<core:ListItem key="{settore}" text="{settore}" additionalText="{settoredescr}"/>
														</ComboBox>
													</VBox>
												</HBox>
											</VBox>
											<ScrollContainer width="100%" height="81%" vertical="true" horizontal="false"
												class="smallScrollBarAltRoute smallpaddingSoItems bordered borderSoItems">
												<!-- Opern order items -->
												<List items="{path:'TRADEITEMS>/' , length: 9000}" id="tradeItemsList">
													<items>
														<CustomListItem type="Active" press=".selectTradeItemGlobal">
															<HBox height="4em" alignItems="Center" width="100%">
																<VBox width="5%" justifyContent="Center"></VBox>
																<VBox width="60%" justifyContent="Center" class="padright05">
																	<ObjectIdentifier title="{TRADEITEMS>productcode}" text="{TRADEITEMS>productdescr}" titleActive="true" titlePress=".selectTradeItem"/>
																</VBox>
																<VBox width="35%">
																	<HBox visible="{TRADEITEMS>visibleFields}">
																		<Text text="box:" class="labelGreySmall" width="2.5em"></Text>
																		<Text text="{path:'TRADEITEMS>totalCP',formatter:'.truncateNumber'}"></Text>
																	</HBox>
																	<HBox visible="{TRADEITEMS>visibleFields}">
																		<Text text="KG:" class="labelGreySmall" width="2.5em"></Text>
																		<Text text="{path:'TRADEITEMS>totalKg',formatter:'.truncateNumber'}"></Text>
																	</HBox>
																</VBox>
															</HBox>
														</CustomListItem>
													</items>
												</List>
											</ScrollContainer>
										</VBox>
									</Page>
